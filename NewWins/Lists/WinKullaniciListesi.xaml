<Window x:Class="ExtremeTaleplerV2.NewWins.Lists.WinKullaniciListesi"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ExtremeTaleplerV2.NewWins.Lists"
        xmlns:dgx="clr-namespace:DataGridExtensions;assembly=DataGridExtensions"
        mc:Ignorable="d"
        Title="Kullanıcı Listesi" FontFamily="Bahnschrift" Height="300" Width="350" WindowStartupLocation="CenterScreen" ResizeMode="NoResize" Icon="/NewWins/Lists/Request Service.png">
    <Grid>
        <DataGrid x:Name="dgKullanici"
          dgx:DataGridFilter.IsAutoFilterEnabled="True"
          IsReadOnly="True" MouseDoubleClick="dgKullanici_MouseDoubleClick"
          HorizontalGridLinesBrush="LightGray" VerticalGridLinesBrush="LightGray" RowHeight="24"
          >
        </DataGrid>
    </Grid>
</Window>
